<h1><%= board.escape("title") %></h1>


<ul id="sortable">
  <% board.lists().each(function (list) { %>
    <li>
      <%= list.escape('title') %>
    </li>
  <% }); %>
</ul>

<a href="#/">Back to Index</a>

<script>
$(function() {
  $( "#sortable" ).sortable();
  $( "#sortable" ).disableSelection();
});
</script>